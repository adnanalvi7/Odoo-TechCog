<odoo>
    <!-- Extend field access rights -->
    <record model="ir.model.access" id="access_hr_employee_has_timesheet">
        <field name="name">hr.employee has_timesheet access</field>
        <field name="model_id" ref="hr.model_hr_employee"/>
        <field name="group_id" ref="hr_timesheet.group_hr_timesheet_user"/>
        <field name="perm_read">1</field>
        <field name="perm_write">0</field>
    </record>
    
    <record model="ir.model.access" id="access_hr_employee_calendar_mismatch">
        <field name="name">hr.employee calendar_mismatch access</field>
        <field name="model_id" ref="hr.model_hr_employee"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read">1</field>
        <field name="perm_write">0</field>
    </record>
</odoo>